  "main": "dist/ng1bs5.js",
  "module": "src/index.js",
  "scripts": {
    "build:demo": "webpack --config webpack.es6-demo.js --mode production",
    "dev:demo": "webpack serve --config webpack.es6-demo.js --mode development --open",
    "watch:demo": "webpack --config webpack.es6-demo.js --mode development --watch",
    "start": "webpack serve --config webpack.es6-demo.js --open --mode development",
    "build": "webpack --mode production",
